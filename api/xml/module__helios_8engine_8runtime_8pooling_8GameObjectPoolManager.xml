        <qualifiedname>helios::engine::runtime::pooling::GameObjectPoolManager::fillPool</qualifiedname>
        <param>
          <type>const <ref refid="structhelios_1_1engine_1_1core_1_1data_1_1GameObjectPoolId" kindref="compound">helios::engine::core::data::GameObjectPoolId</ref></type>
